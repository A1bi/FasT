<% title = "Mitglieder-Login" %>
<div class="hl">Mitglieder-Login</div>
<%= form_tag members_login_path do %>
<table class="rounded panel">
	<tr>
		<td>e-mail-Adresse:</td>
		<td><%= text_field_tag :email, params[:email] %></td>
	</tr>
	<tr>
		<td>Passwort:</td>
		<td><%= password_field_tag :password %></td>
	</tr>
</table>
<div class="submit">
	<%= submit_tag "login" %>
</div>
<% end %>
