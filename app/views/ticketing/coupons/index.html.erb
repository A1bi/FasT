<% title "Gutscheine" %>
<% include_css "ticketing/coupons" %>
<% include_js "ticketing/base" %>
<%= render "new_button", action: "Gutschein erstellen", url: new_ticketing_coupon_path %>
<% cache [:ticketing, :coupons, @coupons] do %>
  <%= render "index_table", coupons: @coupons, title: "Gutscheine" %>
<% end %>
<% cache [:ticketing, :coupons, :expired, @coupons_expired] do %>
  <%= render "index_table", coupons: @coupons_expired, title: "Abgelaufene Gutscheine" %>
<% end %>