<% local_assigns.fetch(:tickets, @tickets).each do |ticket| %>
      „<%= ticket.event.name %>“ | <%= l ticket.date.date, format: :long_weekday %> | <%= ticket.type.name %> | <% if ticket.seat.present? %>Sitz <%= ticket.seat.full_number %><% else %>Freie Platzwahl<% end %> | <%= number_to_currency(ticket.price) %>
<% end %>
